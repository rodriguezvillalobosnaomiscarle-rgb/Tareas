calif=[]
nombres=[]
nommayor=[]
def leer (c,n):
    for i in range(10):
        n.append(input("dime tu nombre"))
        c.append(float(input("dame la calificacion")))

def alumno_mayor(c,n,nm):
    for i in range(len(c)):
        if c[i]>8.5:
            nm.append(n[i])
def resultado(nm):
    print("alumnos con mayor promedio")
    for i in nm:
        print(f"{i}")

leer(calif, nombres)
alumno_mayor(calif, nombres, nommayor)
resultado(nommayor)
edades=[]
mayoredad=[]
menoredad=[]
def leer (e):
    for i in range(10):
        e.append(int(input("dime tu edad: ")))
def person_mayor(e,my,mn):
    for edad in e:
        if edad >= 18:
            my.append(edad)
        else:
            mn.append(edad)
    print("Los mayores de edad son:")
    for i in my:
        print(f"{i}")
    print("los mayores de edad son:", len(my))
    print("los menores de edad son:", len(mn))


leer(edades)
person_mayor(edades, mayoredad, menoredad)
resultado(mayoredad, menoredad)